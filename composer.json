{
    "name": "himanverma/mailjet-laravel-driver",
    "description": "MailJet driver for laravel 5.5 and above",
    "keywords": [
        "laravel",
        "mail",
        "mailjet"
    ],
    "homepage": "https://github.com/himanverma/mailjet-laravel-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Himanshu Verma",
            "email": "himan.verma@live.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~6.0",
        "illuminate/mail": "~6.0",
        "mailjet/mailjet-apiv3-php": ">=1.2.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0.0",
        "phpunit/phpunit": "~8.0",
        "orchestra/testbench": "~4.0",
        "themsaid/laravel-mail-preview": "2.*"
    },
    "autoload": {
        "psr-4": {
            "MailjetLaravelDriver\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "MailjetLaravelDriver\\MailJetServiceProvider"
            ]
        }
    }
}
